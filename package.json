{
  "name": "ja-cms",
  "version": "1.0.0",
  "description": "Modern Content Management System dengan Node.js + Express.js + Next.js + Prisma",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "cd backend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend && npm start",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "db:setup": "cd backend && npm run db:setup",
    "db:migrate": "cd backend && npm run db:migrate",
    "db:seed": "cd backend && npm run db:seed",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:backend": "jest --selectProjects=backend",
    "test:frontend": "jest --selectProjects=frontend",
    "test:integration": "jest --selectProjects=integration",
    "test:e2e": "jest --selectProjects=e2e",
    "test:backend:watch": "jest --selectProjects=backend --watch",
    "test:frontend:watch": "jest --selectProjects=frontend --watch",
    "test:integration:watch": "jest --selectProjects=integration --watch",
    "test:e2e:watch": "jest --selectProjects=e2e --watch",
    "lint": "npm run lint:backend && npm run lint:frontend",
    "lint:backend": "cd backend && npm run lint",
    "lint:frontend": "cd frontend && npm run lint",
    "format": "npm run format:backend && npm run format:frontend",
    "format:backend": "cd backend && npm run format",
    "format:frontend": "cd frontend && npm run format",
    "type-check": "npm run type-check:backend && npm run type-check:frontend",
    "type-check:backend": "cd backend && npm run type-check",
    "type-check:frontend": "cd frontend && npm run type-check",
    "clean": "rm -rf node_modules && rm -rf backend/node_modules && rm -rf frontend/node_modules && rm -rf shared/node_modules",
    "clean:build": "rm -rf backend/dist && rm -rf frontend/.next && rm -rf shared/dist",
    "clean:all": "npm run clean && npm run clean:build",
    "reinstall": "npm run clean && npm install"
  },
  "keywords": [
    "cms",
    "nodejs",
    "express",
    "nextjs",
    "prisma",
    "postgresql",
    "typescript",
    "modern",
    "content-management"
  ],
