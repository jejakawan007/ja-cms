{
  "name": "ja-cms-frontend",
  "version": "1.0.0",
  "description": "Frontend untuk JA-CMS dengan Next.js + TypeScript",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:turbo": "next dev -p 3000 --turbo",
    "dev:clean": "rm -rf .next && next dev -p 3000",
    "dev:turbo:clean": "rm -rf .next && next dev -p 3000 --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf .next node_modules/.cache"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "react-grid-layout": "^1.5.2"
  },
  "devDependencies": {
    "@types/react-grid-layout": "^1.3.5"
  }
}
